{
    "name": "jacksleight/laravel-omni",
    "autoload": {
        "psr-4": {
            "JackSleight\\LaravelOmni\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JackSleight\\LaravelOmni\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JackSleight\\LaravelOmni\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.3|^8.4",
        "laravel/framework": "^12.0",
        "livewire/livewire": "^3.6"
    },
    "require-dev": {
        "laravel/pint": "^1.20",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "orchestra/testbench": "^10.0"
    },
    "scripts": {
        "test": "pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
